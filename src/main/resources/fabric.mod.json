{
	"schemaVersion": 1,
	"id": "veinbuddy",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"sbobicus",
		"Protonull",
		"VastInfiniteNet"
	],
	"contact": {
		"homepage": "${mod_home_url}",
		"sources": "${mod_source_url}",
		"issues": "${mod_issues_url}"
	},
	"license": "${copyright_licence}",
	"icon": "assets/veinbuddy/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"com.github.sbobicus.VeinBuddyClient"
		]
	},
	"depends": {
		"java": ">=21",
		"minecraft": "${minecraft_version}",
		"fabricloader": ">=${fabric_loader_version}",
		"fabric-api": "*"
	},
	"mixins": [
		"veinbuddy.mixins.json"
	]
}
